{
  "name": "vhs recorder",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Recorder for vhs",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "record!"
  },
  "web_accessible_resources": [
    "jquery.js", "vhs.js"
  ],
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
