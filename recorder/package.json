  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siddharthkp/vhs/issues"
  },
  "homepage": "https://github.com/siddharthkp/vhs#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
